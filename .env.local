# ======================
# PRESERVE EXISTING CONFIG
# ======================
NEXT_PUBLIC_API_URL="https://ai-affiliate-backend.onrender.com"
NEXT_PUBLIC_APP_ENV="production"

# ======================
# QUANTUM AI CORE CONFIG
# ======================
# Required for Quantum initialization
QUANTUM_API_KEY="qai_prod_${YOUR_SECRET_KEY}"
NEXT_PUBLIC_QUANTUM_MODE="production"

# Quantum Network Settings
QUANTUM_GATEWAY="https://gateway.quantum-ai.network"
QUANTUM_ENTANGLEMENT_ENABLED="true"

# ======================
# REVENUE OPTIMIZATION
# ======================
# Web Monetization (ILP)
NEXT_PUBLIC_WEB_MONETIZATION_POINTER="$ilp.uphold.com/YOUR_POINTER"

# Affiliate Network
AFFILIATE_NETWORK_ID="quantum_aff_${YOUR_NETWORK_ID}"
AFFILIATE_FALLBACK_URL="https://backup.affiliate-network.com"

# ======================
# SELF-HEALING CONFIG
# ======================
QUANTUM_AUTO_REPAIR="true"
QUANTUM_HEALING_INTERVAL="300000" # 5 minutes
QUANTUM_FALLBACK_ENABLED="true"

# ======================
# PERFORMANCE TUNING
# ======================
QUANTUM_CACHE_TTL="20000" # 20 seconds
QUANTUM_MAX_ENTANGLEMENTS="5"
