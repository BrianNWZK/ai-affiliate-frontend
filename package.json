{
  "name": "quantum-ai-affiliate-pro",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_QUANTUM_MODE=development next dev",
    "build": "NEXT_PUBLIC_QUANTUM_MODE=production next build",
    "start": "NEXT_PUBLIC_QUANTUM_MODE=production next start",
    "analyze": "ANALYZE=true next build",
    "quantum": "NEXT_PUBLIC_QUANTUM_MODE=debug next dev",
    "lint": "next lint",
    "quantum-simulate": "node scripts/quantum-runner.js",
    "quantum-test": "node scripts/quantum-test.js",
    "monetize": "node scripts/quantum-monetization.js",
    "optimize-revenue": "node scripts/revenue-optimizer.js"
  },
  "dependencies": {
    "next": "13.5.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.16.0",
    "react-intersection-observer": "^9.5.0",
    "crypto-js": "^4.1.1",
    "node-cache": "^5.1.2",
    "lru-cache": "^7.18.3"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27"
  },
  "quantum": {
    "entanglement": {
      "enabled": true,
      "autoScale": true,
      "maxNodes": 5
    },
    "neuralCache": {
      "enabled": true,
      "ttl": 20000,
      "learningRate": 0.9
    },
    "monetization": {
      "streams": [
        "affiliate",
        "ads",
        "data",
        "content",
        "services"
      ],
      "autoOptimize": true
    },
    "telemetry": {
      "performance": {
        "enabled": true,
        "interval": 5000
      },
      "errorTracking": {
        "enabled": true,
        "reportTo": "quantum-ai"
      },
      "revenueAnalytics": {
        "enabled": true,
        "realTime": true
      }
    }
  }
}
